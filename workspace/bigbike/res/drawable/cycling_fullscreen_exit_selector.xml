    </item>
    
    <item>
		<shape>
			<stroke  
                android:width="1dip"
                android:color="#ffffff" />
		    <solid android:color="#00000000"/>
		    <corners 
		        android:radius="18dip" />
		</shape>
    </item>
    
</selector>